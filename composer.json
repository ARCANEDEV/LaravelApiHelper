{
    "name":        "arcanedev/laravel-api-helper",
    "description": "API Helper for Laravel",
    "keywords":    ["arcanedev", "laravel", "api", "json", "middleware", "response", "ajax"],
    "homepage":    "https://github.com/ARCANEDEV/LaravelApiHelper",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=7.1.3",
        "ext-json":          "*",
        "arcanedev/support": "~4.4.0"
    },
    "require-dev": {
        "phpunit/phpunit":     "~7.0",
        "phpunit/phpcov":      "~5.0",
        "orchestra/testbench": "~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelApiHelper\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelApiHelper\\Tests\\": "tests/"
        }
    }
}
